            function isWsOpen(ws) { return ws.readyState === ws.OPEN }
            function wsSend(sendObj, ws) {
                if (isWsOpen(ws)) {
                    ws.send(JSON.stringify(sendObj))
                } else {
                    console.log("ws is not open")
